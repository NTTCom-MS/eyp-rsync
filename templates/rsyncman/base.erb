[rsyncman]
<% if defined?(@mail_to) -%>
to=<%= @mail_to %>
<% end -%>
<% if defined?(@host_id) -%>
host-id=<%= @host_id %>
<% end -%>
logdir=<%= @logdir %>
<% if defined?(@pre_script) -%>
pre-script=<%= @pre_script %>
<% end -%>
<% if defined?(@post_script) -%>
post-script=<%= @post_script %>
<% end -%>
